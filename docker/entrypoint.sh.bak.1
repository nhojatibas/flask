#!/bin/sh

env > _log.txt
echo "DB_INIT.....: $DB_INIT"  >> _log.txt
echo "DB_MIGRATE..: $DB_MIGRATE" >> _log.txt
echo "DB_UPGRATE.,: $DB_UPGRADE" >> _log.txt

[ "$DB_INIT" = 'True' ] && python3 run.py init
[ "$DB_MIGRATE" = 'True' ] && python3 run.py migrate
[ "$DB_UPGRADE" = 'True' ] && python3 run.py upgrade

#python run.py init 
#python3 run.py

/bin/sh
